{
  "name": "template",
  "version": "1.0.0",
  "description": "Basic project template with tests and coverage",
  "main": "index.js",
  "scripts": {
    "test": "nyc node_modules/mocha/bin/_mocha **/*Test.js"
  },
  "nyc": {
    "all": true,
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 95,
    "branches": 95,
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "exclude": [
      "test/**/*.js",
      "coverage"
    ]
  },
  "author": "Micha≈Ç Mania",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-mocha": "^5.3.0",
    "mocha": "^5.2.0",
    "nyc": "^13.2.0",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.3"
  }
}
